{
    "pretrained_weight":  "roberta-large",
    "dataset": "mnli",
    "max_text_length": 256,
    "batch_size": 8,
    "epochs":6,
    "eval_batch_size": 1,
    "num_batches": 1000,
    "max_num_lbl_tok": 1,
